    }
  },
  "flex-layout.row#breadcrumb-container": {
    "children": [
      "breadcrumb.search#bread-plp"
    ]
  },
  "flex-layout.row#search.buttons": {
    "props": {
      "preserveLayoutOnMobile": true,
      "colSizing": "auto",
      "fullWidth": true,
      "blockClass": "filter-navigation"
    },
    "children": [
      // "total-products.v2",
      "order-by.v2"
    ]
